        <rtc:Configuration xsi:type="rtcExt:configuration_ext" rtcExt:variableName="PF_max_loglikelihood_dyn_range" rtc:unit="" rtc:defaultValue="15" rtc:type="double" rtc:name="PF_max_loglikelihood_dyn_range">
