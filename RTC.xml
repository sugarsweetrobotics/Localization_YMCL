        </rtc:Configuration>
        <rtc:Configuration xsi:type="rtcExt:configuration_ext" rtcExt:variableName="PF_AuxFilterStandard_FirstStageWeightsMonteCarlo" rtc:unit="" rtc:defaultValue="false" rtc:type="string" rtc:name="PF_AuxFilterStandard_FirstStageWeightsMonteCarlo">
